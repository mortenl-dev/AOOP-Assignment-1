<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:AOOPAssignment1.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="AOOPAssignment1.Views.MainWindow"
        x:DataType="vm:MainWindowViewModel"
        Icon="/Assets/avalonia-logo.ico"
        Title="AOOPAssignment1">

    
        <StackPanel Name="Panel" HorizontalAlignment="Center" VerticalAlignment="Center" Spacing="10">
        <Button Content="Open File" Click="OpenFile_Click"/>
        <Button Content="Save File" Click="SaveFile_Click"/>
        <Button Content="Flip horizontally" Click="Flip_Horizontally"/>
        <Button Content="Flip vertically" Click="Flip_Vertically"/>
    </StackPanel>
        
    

</Window>

